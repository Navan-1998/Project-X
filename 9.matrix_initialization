//Write a program to Make and Add Values to the Matrix
using System;
using System.Collections.Generic;
using System.Linq;
using System.Net;
using System.Runtime.Remoting.Services;
using System.Text;
using System.Threading.Tasks;

namespace Array1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[,] matrix1 = new int[3,3];
            Console.WriteLine("Enter Integers to the Matrix");
            for (int i = 0;i<3;i++)
            {
                for (int j = 0;j<3;j++)
                {
                    matrix1[i,j]=int.Parse(Console.ReadLine());
                }
            }
            Console.WriteLine("Entered Matrix is:");
            for (int i = 0; i < 3; i++)
            {
                for (int j = 0; j < 3; j++)
                {
                    Console.Write($"{matrix1[(int)i,j]}  ");
                }
                Console.WriteLine();
            }
            Console.ReadKey();
        }
    }
}
